{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "airbnb/hooks"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "func-names": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-return-await": "off",
    "no-underscore-dangle": "off",
    "react/jsx-props-no-spreading": "off"
  }
}